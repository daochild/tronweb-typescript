  "license": "MIT",
  "devDependencies": {
    "@noble/secp256k1": "^2.1.0",
    "@types/node": "^22.5.4",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "tronweb": "^5.3.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daochild/tronweb-typescript"
