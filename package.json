{
  "name": "@daochild/tronweb-typescript",
  "version": "1.2.0",
  "description": "Typescript support package for tronweb library ^4.x",
  "types": "./dist/index",
  "type": "module",
  "keywords": [
    "tron",
    "tronweb",
    "typescript",
    "tronweb-typescript",
    "tronweb-ts",
    "types-tronweb"
  ],
  "scripts": {
    "watch": "npx nodemon -w ./src -e *.ts --exec npx tsx ./src/index.ts",
    "build": "rimraf ./dist && tsc",
    "test": "npx tsx ./tests/test.ts"
  },
  "contributors": [
    {
      "name": "Pavlo Bolhar",
      "url": "https://github.com/daochild",
